<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="objects\hollow.obj" />
    <None Remove="objects\hollowknight.obj" />
    <None Remove="objects\sword.obj" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="objects\hollow.obj" />
    <Content Include="objects\hollowknight.obj" />
    <Content Include="objects\sword.obj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="7.0.0" />
    <PackageReference Include="System.IO.Ports" Version="7.0.0" />
  </ItemGroup>

</Project>
